package BasicArray;

import java.util.Scanner;

public class Array4_input {
	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		
		int array[]=new int[6];
		
		System.out.println("Enter values for array: ");
		for(int i=0; i<array.length; i++);
		int i;
		array[i]=sc.nextInt();
	}

}
