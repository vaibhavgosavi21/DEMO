package BasicArray;

public class Array2 {
	public static void main(String[] args) {
		int marks[]=new int [10];    // array of  fixed size 10
		
		marks[0]=90;
		marks[1]=87;
		marks[3]=45;
		marks[2]=22;
		marks[4]=56;
		System.out.println("array size: "+marks[0]);
		System.out.println("array size: "+marks[1]);
		System.out.println("array size: "+marks[2]);
		System.out.println("array size: "+marks[3]);
		System.out.println("array size: "+marks[4]);

		

	}

}
