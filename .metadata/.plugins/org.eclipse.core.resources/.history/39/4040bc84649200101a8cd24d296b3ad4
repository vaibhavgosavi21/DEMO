package BasicArray;

public class Arr1 {
	
	public static void main(String[] args) {
		
		int numbers[]= {1,4,6,33,55};  //array
		
		System.out.println(numbers.length);  //array lenght
		
		System.out.println(numbers[0]); //print array value st index 0
		System.out.println(numbers[1]); //print array value st index 1
		System.out.println(numbers[2]); //print array value st index 2
		System.out.println(numbers[3]); //print array value st index 3
		System.out.println(numbers[4]); //print array value st index 4

	}

}
